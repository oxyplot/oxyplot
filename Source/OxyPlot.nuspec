<?xml version="1.0"?>
<package>
  <metadata>
    <id>OxyPlot</id>
    <title>OxyPlot for WPF/Windows8/Windows Phone/Windows Forms/Silverlight</title>
    <version>0.0.0</version>
    <authors>Oystein Bjorke</authors>
    <description>OxyPlot is a plotting library for .NET. This package contains the components for WPF, Windows Universal, Silverlight, Windows Forms and Windows Phone Silverlight. The Xamarin components are not included in this package.</description>
    <releaseNotes></releaseNotes>
    <licenseUrl>https://raw.githubusercontent.com/oxyplot/oxyplot/master/LICENSE</licenseUrl>
    <projectUrl>http://oxyplot.org/</projectUrl>
    <iconUrl>http://resources.oxyplot.org/icons/oxyplot_128.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>plotting plot charting chart</tags>
  </metadata>
  <files>
    <file src="..\Output\PCL\OxyPlot.???" target="lib\" />

    <file src="..\Output\NET45\OxyPlot.Wpf.???" target="lib\net45" />
    <file src="..\Output\NET40\OxyPlot.Wpf.???" target="lib\net40" />    
    <file src="..\Output\NET45\OxyPlot.Xps.???" target="lib\net45" />
    <file src="..\Output\NET40\OxyPlot.Xps.???" target="lib\net40" />

    <file src="..\Output\WindowsUniversal\OxyPlot.WindowsUniversal.*" target="lib\portable-windows8+wpa81\OxyPlot.WindowsUniversal\" />
    <file src="..\Output\WindowsUniversal\Themes\*.*" target="lib\portable-windows8+wpa81\OxyPlot.WindowsUniversal\OxyPlot.WindowsUniversal\Themes\" />

    <file src="..\Output\SL5\OxyPlot.Silverlight.???" target="lib\sl5" />

    <file src="..\Output\NET45\OxyPlot.WindowsForms.???" target="lib\net45" />
    <file src="..\Output\NET40\OxyPlot.WindowsForms.???" target="lib\net40" />

    <file src="..\Output\WP8\OxyPlot.WP8.???" target="lib\windowsphone8" />

    <file src="..\LICENSE" />
    <file src="..\AUTHORS" />
    <file src="..\CONTRIBUTORS" />
    <file src="..\README.md" />
  </files>
</package>
